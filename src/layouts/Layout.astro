---
import SiteHead from '../components/SiteHead.astro'
import Header from '../components/Header.astro'
import Footer from '../components/Footer.astro'

export interface Props {
	title: string
}

const { title } = Astro.props
---

<!DOCTYPE html>
<html lang="en">
	<SiteHead title={title} />
	<body>
    <Header />
    <main>
      <slot />
    </main>
    <Footer />
	</body>
</html>
